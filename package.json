{
  "name": "pokemon-flood",
  "version": "1.0.0",
  "description": "Browser-based Pokemon game taking place in the prehistory of licensed Pokeon games",
  "main": "index.js",
  "private": true,
  "scripts": {
    "reset": "rm -rf dist && rm -rf node_modules && npm i",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && webpack",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,scss,html",
      "quiet": true
    }
  },
  "keywords": [
